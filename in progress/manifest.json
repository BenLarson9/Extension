{
	"manifest_version": 3,
	"name": "Pagedit Red",
	"version": "1.0.0",
	"description": "",
	"icons": {"16": "logodef.png",
		  "48": "logo.png"},


	"content_scripts": [
		{
			"matches": [ "*://*.google.com/" ],
			"js": [ "script.js" ],
			"css": [ "run.css" ]

		}
	],

	


	"permissions": ["activeTab"],

	"action":{
		"default_icon": "logodef.png",
		"default_popup": "popup.html",
		"default_title": "Latest Report"
	}

}